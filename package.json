{
  "name": "dhm-dashboard",
  "version": "0.0.0",
  "main": "index.js",
  "author": "Alorel",
  "license": "MIT",
  "scripts": {
    "lint": "tslint --fix -p tsconfig.json",
    "preprocess-externals": "ts-node -P tsconfig.cjs.json preprocess-externals.ts",
    "prebuild": "rimraf dist/* && npm --scripts-prepend-node-path=auto run preprocess-externals",
    "build": "webpack --colors --progress",
    "build:prod": "cross-env WP_MINIMISE=1 npm --scripts-prepend-node-path=auto run build",
    "postbuild:prod": "ts-node -P tsconfig.cjs.json postprocess.ts",
    "watch": "npm --scripts-prepend-node-path=auto run build -- --watch"
  },
  "devDependencies": {
    "@alorel-personal/tslint-rules": "^4.1.1",
    "@types/bluebird": "^3.5.25",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.120",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.6.3",
    "bluebird": "^3.5.3",
    "cross-env": "^5.2.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.14",
    "lodash-es": "^4.17.14",
    "moment": "^2.24.0",
    "node-sass": "^7.0.0",
    "raw-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "rxjs": "~6.3.3",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "typescript-lazy-get-decorator": "^1.2.2",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "wrapper-webpack-plugin": "^2.1.0"
  }
}
